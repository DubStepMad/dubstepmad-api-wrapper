{
  "name": "@dubstepmad/dubstepmad-api-wrapper",
  "version": "1.2.2",
  "description": "TypeScript wrapper for the DubstepMad API",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts",
    "dev": "npm run clean && tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "node-fetch": "^3.3.2"
  }
}
