{
  "name": "@dubstepmad/dubstepmad-api-wrapper",
  "version": "1.0.0",
  "description": "TypeScript wrapper for the DubstepMad API",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "keywords": [
    "dubstepmad",
    "api",
    "wrapper",
    "music"
  ],
  "author": "DubStepMad",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1"
  }
}
